{
  "name": "react-antd-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "craco build",
    "eject": "react-scripts eject",
    "ncu": "npm-check-updates",
    "ncu:u": "npm-check-updates -u",
    "sort:pkg": "sort-package-json",
    "start": "craco start",
    "test": "craco test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@emotion/react": "^11.7.1",
    "ahooks": "^3.1.3",
    "antd": "^4.18.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1"
  },
  "devDependencies": {
    "@craco/craco": "^6.4.3",
    "@emotion/babel-plugin": "^11.7.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.6",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.5",
    "npm-check-updates": "^12.1.0",
    "prettier": "^2.5.1",
    "react-scripts": "4.x",
    "sort-package-json": "^1.53.1",
    "typescript": "^4.5.4",
    "web-vitals": "^2.1.2"
  }
}
